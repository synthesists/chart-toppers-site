{
  "name": "calculator",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "deploy": "sls",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "test:e2e": "yarn cypress run -P tests/e2e",
    "test:e2e:open": "yarn cypress open -P tests/e2e",
    "test:unit": "jest",
    "test": "yarn test:unit && yarn test:e2e",
    "test:coverage": "yarn test:unit --coverage",
    "test:watch": "yarn test:unit --watchAll",
    "test:full": "yarn lint && yarn test:coverage && yarn test:e2e"
  },
  "license": "MIT",
  "devDependencies": {
    "@testing-library/dom": "^7.26.4",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/cypress": "^1.1.3",
    "@types/faker": "^5.1.4",
    "@types/jest": "^26.0.3",
    "@types/next": "^9.0.0",
    "@types/node": "^14.14.2",
    "@types/react": "^16.9.53",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "cypress": "^5.4.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "faker": "^5.1.0",
    "husky": "^4.3.0",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "serverless": "^2.8.0",
    "ts-jest": "^26.4.1",
    "ts-node": "^8.10.2",
    "typescript": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test:full"
    }
  },
  "dependencies": {
    "axios": "^0.21.0",
    "next": "^9.5.5",
    "node-sass": "^4.14.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
